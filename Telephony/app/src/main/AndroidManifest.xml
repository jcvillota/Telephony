<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.ucucite.telephony">

    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Telephony">
        <activity android:name=".call5"></activity>
        <activity android:name=".message5" />
        <activity android:name=".call4" />
        <activity android:name=".message4" />
        <activity android:name=".call3" />
        <activity android:name=".message3" />
        <activity android:name=".call2" />
        <activity android:name=".message2" />
        <activity android:name=".call" />
        <activity android:name=".message" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>